model Account {
  id                    String    @id @default(auto()) @map("_id") @db.ObjectId
  userId                String    @db.ObjectId
  accountId              String
  providerId            String
  accessToken            String?
  refreshToken           String?
  accessTokenExpiresAt  DateTime?
  refreshTokenExpiresAt  DateTime?
  scope                 String?
  idToken               String?
  password              String?
  createdAt             DateTime   @default(now())
  updatedAt             DateTime   @updatedAt

  @@map("accounts")
}
